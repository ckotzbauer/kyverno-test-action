{
  "name": "kyverno-test-action",
  "version": "2.0.1",
  "private": true,
  "description": "TypeScript template action",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build src/main.ts -o dist",
    "test": "jest",
    "all": "npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckotzbauer/kyverno-test-action.git"
  },
  "keywords": [
    "actions",
    "kyverno",
    "check"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/exec": "1.1.1",
    "@actions/glob": "0.4.0",
    "@actions/io": "1.1.3",
    "@actions/tool-cache": "2.0.1",
    "@octokit/graphql": "7.0.1",
    "axios": "1.4.0",
    "is-url": "1.2.4",
    "semver": "7.5.4"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/is-url": "1.2.30",
    "@types/jest": "29.5.3",
    "@types/js-yaml": "4.0.5",
    "@types/node": "18.17.2",
    "@types/semver": "7.5.0",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "@vercel/ncc": "0.36.1",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.6.2",
    "js-yaml": "4.1.0",
    "prettier": "3.0.0",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  }
}
