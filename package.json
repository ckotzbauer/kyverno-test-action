{
  "name": "kyverno-test-action",
  "version": "2.0.1",
  "private": true,
  "description": "TypeScript template action",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build src/main.ts -o dist",
    "test": "jest",
    "all": "npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckotzbauer/kyverno-test-action.git"
  },
  "keywords": [
    "actions",
    "kyverno",
    "check"
  ],
  "author": "",
  "license": "MIT",
  "overrides": {
    "semver": "7.7.2"
  },
  "dependencies": {
    "@actions/core": "1.11.1",
    "@actions/exec": "1.1.1",
    "@actions/glob": "0.5.0",
    "@actions/io": "1.1.3",
    "@actions/tool-cache": "2.0.2",
    "@octokit/graphql": "7.1.1",
    "axios": "1.12.2",
    "is-url": "1.2.4",
    "semver": "7.7.2"
  },
  "devDependencies": {
    "@types/axios": "0.14.4",
    "@types/is-url": "1.2.32",
    "@types/jest": "29.5.14",
    "@types/js-yaml": "4.0.9",
    "@types/node": "22.18.6",
    "@types/semver": "7.7.1",
    "@typescript-eslint/eslint-plugin": "8.44.1",
    "@typescript-eslint/parser": "8.44.1",
    "@vercel/ncc": "0.38.4",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "jest": "29.7.0",
    "js-yaml": "4.1.0",
    "prettier": "3.6.2",
    "ts-jest": "29.4.4",
    "typescript": "5.9.2"
  }
}
