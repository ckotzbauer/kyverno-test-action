{
  "name": "kyverno-test-action",
  "version": "2.0.1",
  "private": true,
  "description": "TypeScript template action",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build src/main.ts -o dist",
    "test": "jest",
    "all": "npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckotzbauer/kyverno-test-action.git"
  },
  "keywords": [
    "actions",
    "kyverno",
    "check"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.9.1",
    "@actions/exec": "1.1.1",
    "@actions/glob": "0.3.0",
    "@actions/io": "1.1.2",
    "@actions/tool-cache": "2.0.1",
    "@octokit/graphql": "5.0.1",
    "axios": "0.27.2",
    "is-url": "1.2.4",
    "semver": "7.3.7"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/is-url": "1.2.30",
    "@types/jest": "29.0.3",
    "@types/js-yaml": "4.0.5",
    "@types/node": "16.11.60",
    "@types/semver": "7.3.12",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "@vercel/ncc": "0.34.0",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.0.3",
    "js-yaml": "4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "29.0.1",
    "typescript": "4.8.3"
  }
}
