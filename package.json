{
  "name": "kyverno-test-action",
  "version": "1.0.2",
  "private": true,
  "description": "TypeScript template action",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build src/main.ts -o dist",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckotzbauer/kyverno-test-action.git"
  },
  "keywords": [
    "actions",
    "kyverno",
    "check"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.6.0",
    "@actions/exec": "1.1.0",
    "@actions/glob": "0.2.0",
    "@actions/io": "1.1.1",
    "@actions/tool-cache": "1.7.1",
    "@octokit/graphql": "4.8.0",
    "axios": "0.26.0",
    "is-url": "1.2.4",
    "semver": "7.3.5"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/is-url": "1.2.30",
    "@types/jest": "27.4.1",
    "@types/js-yaml": "4.0.5",
    "@types/node": "16.11.26",
    "@types/semver": "7.3.9",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "@vercel/ncc": "0.33.3",
    "eslint": "8.10.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.5.1",
    "js-yaml": "4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "typescript": "4.6.2"
  }
}
